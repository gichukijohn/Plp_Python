my_list=[]; # creating empty list
my_list=[10, 20, 30, 40]; #appending elements
my_list.insert(1,15) # adding element at second place
my_list2=[50, 60, 70]
my_list.extend(my_list2) #combining elements
del my_list[-1] # deleting elements
my_list.sort() # arranging elements in ascending order
print(my_list)
print(my_list.index(30))  # finding and printing index of 30
